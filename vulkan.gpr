library project Vulkan is

   for Source_Dirs use ("src/**");
   for Object_Dir use ".obj/" & project'name;
   for library_name use "a-vulkan";
   for Library_dir use "lib";

   package Compiler is
      for Default_Switches ("ada") use ("-gnatwa", "-gnatQ", "-gnat12");
   end Compiler;

end Vulkan;
